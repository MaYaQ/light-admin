{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vben/ts-config/vue-app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "types": ["vite/client"],
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["types/*"],
      "@c/*":["packages/components/*"],
      "@h/*":["packages/myhooks/*"],
      "@utils/*":["packages/utils/*"],
      "@hooks/*":["packages/hooks/*"],
      "@layouts/*":["packages/layouts/*"],
      "@directives/*":["packages/directives/*"],
      "@logics/*":["packages/logics/*"],
      "@design/*":["packages/design/*"],
      "@enums/*":["packages/enums/*"],
      "@locales/*":["packages/locales/*"],
      "@settings/*":["packages/settings/*"],
      "@store/*":["packages/store/*"],
      "@router/*":["packages/router/*"],
      "@views/*":["packages/views/*"],
    }
  },
  // "references": [
  //   {
  //     "path": "./tsconfig.node.json"
  //   }

  
 
  "include": [
    "tests/**/*.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "types/**/*.d.ts",
    "types/**/*.ts",
    "build/**/*.ts",
    "build/**/*.d.ts",
    "mock/**/*.ts",
    "vite.config.ts",
    "types/auto-import.d.ts",
    "packages/**/*.ts",
  ],
  "exclude": ["node_modules", "tests/server/**/*.ts", "dist", "**/*.js"]
}
